                      getField('cough_duration.error').html('This field can not be null').show();
                           error=false;
                 }
                
                  if(getValue('cough.value')==1065 &amp;&amp; getValue('haemoptysis.value')==''){
                      getField('haemoptysis.error').html('This field can not be null').show();
