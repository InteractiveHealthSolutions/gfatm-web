function centile_change() {

var sel = document.getElementById("centile_dropdown");
var text= sel.options[sel.selectedIndex].text;
    setValue('weight_percentile.value',text);
}
	 
	 var startDate = new Date();
           var score=0;
           jQuery(function() {
		   var exp1=false,exp2=false,exp3=false,exp4=false,exp5=false,exp6=false,exp7=false,exp8=false,exp9=false,exp10=false;
		   
		      getField('exposure_point1.value').change(function() {
                    if (getValue('exposure_point1.value') == 1065){
						if(exp1==false){
							exp1=true;
                            score = score + 1;       
							setValue('exposure_score.value', score);
						}
                    }
					else{
						if(exp1==true){
							exp1=false;
                            score = score - 1;       
							setValue('exposure_score.value', score);
						}
						else{
						    score = score - 0;       
							setValue('exposure_score.value', score);
						}
						
					}
                });
		getField('exposure_point2.value').change(function() {
					if (getValue('exposure_point2.value') == 1065){
                        	if(exp2==false){
								exp2=true;
								score = score + 1;       
								setValue('exposure_score.value', score);
							}
                    }
