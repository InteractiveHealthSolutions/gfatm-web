				getField('other_co_existing_condition.value').change(function() {
                        var checked= getValue('other_co_existing_condition.value');
                        if(checked == '164235')
                             document.getElementById("other_specify_comorbid_condition").style.display = "block";

                         else{
                             document.getElementById("other_specify_comorbid_condition").style.display = "none";
                             setValue('other_comorbid_conditions.value', '');
}
                });
						
				getField('weight.value').change(function() {
                    if(getValue('height.value')!=''){
						var weight = getValue('weight.value');
						var height = getValue('height.value');
						var finalBmi = weight/(height/100*height/100);
						setValue('bmi.value', finalBmi );
					}
						
                });
				
				getField('height.value').change(function() {
                    if(getValue('weight.value')!=''){
						var weight = getValue('weight.value');
						var height = getValue('height.value');
						var finalBmi = weight/(height/100*height/100);
						setValue('bmi.value', finalBmi );
					}
                });
				
				getField('bmi.value').change(function() {
					if(getValue('weight.value')!='' &amp;&amp; getValue('height.value')!='' ){
						var weight = getValue('weight.value');
						var height = getValue('height.value');
						var finalBmi = weight/(height/100*height/100);
						setValue('bmi.value', finalBmi );
					}
					else{
						setValue('bmi.value', '');
					}
                });
				
				getField('smoking_history.value').change(function() {
					 if (getValue('smoking_history.value') == 1065){
                                                     $j('#smoking_div').show();
                                          } else{
                                                     $j('#smoking_div').hide();
setValue('pack_years.value', '');
setValue('cigarettes_smoked_in_a_day.value', '');
setValue('smoking_duration.value', '');
                                         }
                });
				
				getField('cigarettes_smoked_in_a_day.value').change(function() {
					if(getValue('cigarettes_smoked_in_a_day.value')!='' &amp;&amp; getValue('smoking_duration.value')!='' ){
						if(getValue('cigarettes_smoked_in_a_day.value')== 0 || getValue('smoking_duration.value')== 0 ){
						  	setValue('pack_years.value', 0);

						}
					    else{
						var smoked_per_day = getValue('cigarettes_smoked_in_a_day.value');
						var duration = getValue('smoking_duration.value');
						var finalPackYear = (smoked_per_day /20) / duration ;
						setValue('pack_years.value', finalPackYear);
						}
					}
					else{
						setValue('pack_years.value', '');
					}
                });
				
				getField('smoking_duration.value').change(function() {
					if(getValue('cigarettes_smoked_in_a_day.value')!='' &amp;&amp; getValue('smoking_duration.value')!='' ){
					
					    if(getValue('cigarettes_smoked_in_a_day.value')== 0 || getValue('smoking_duration.value')== 0 ){
						  	setValue('pack_years.value', 0);

						}
					    else{
						var smoked_per_day = getValue('cigarettes_smoked_in_a_day.value');
						var duration = getValue('smoking_duration.value');
						var finalPackYear = (smoked_per_day /20) / duration ;
						setValue('pack_years.value', finalPackYear);
						}
					}
					else{
						setValue('pack_years.value', '');
					}
                });
				
				
				getField('pack_years.value').change(function() {
					if(getValue('cigarettes_smoked_in_a_day.value')!='' &amp;&amp; getValue('smoking_duration.value')!='' ){
						var smoked_per_day = getValue('cigarettes_smoked_in_a_day.value');
						var duration = getValue('smoking_duration.value');
						var finalPackYear = (smoked_per_day /20) / duration ;
						setValue('pack_years.value', finalPackYear);
					}
					else{
						setValue('pack_years.value', '');
					}
                });
				
		   
		   });
	 
	 
	 jQuery(window).bind("load", function() {

			if (getValue('smoking_history.value') == 1065){
						 $j('#smoking_div').show();
			  } else{
						 $j('#smoking_div').hide();
              }
       
        

			<ifMode mode="EDIT">
				
				var x = getValue('weight_percentile.value')
				if(x == '')
				document.getElementById("centile_dropdown").value = '';
				else if(x == 'Between 6-10th Centile')
				document.getElementById("centile_dropdown").value = 'bw_6_10_centile';
				else if(x == 'Between 11-25th Centile')
				document.getElementById("centile_dropdown").value = 'bw_11_25_centile';
				else if(x == 'Between 26-50th Centile')
				document.getElementById("centile_dropdown").value = 'bw_26_50_centile';
				else if(x == 'Between 51-75th Centile')
				document.getElementById("centile_dropdown").value = 'bw_51_75_centile';
				else if(x == 'Between 76-90th Centile')
				document.getElementById("centile_dropdown").value = 'bw_76_90_centile';
				else if(x == 'Between 91-95th Centile')
				document.getElementById("centile_dropdown").value = 'bw_91_95_centile';
				else if(x == 'Between 96-98th Centile')
				document.getElementById("centile_dropdown").value = 'bw_96_98_centile';
				else if(x == '> 98th Centile')
				document.getElementById("centile_dropdown").value = 'greater_then_98_centile';
				else if(x == '&lt;= 5th Centile')
				document.getElementById("centile_dropdown").value = 'less_than_5_centile';
									 
			</ifMode>
			
			var patientage= <lookup expression="patient.age"/>
            if(patientage &lt;0 || patientage &gt;5){
                         $j('#muac_div').hide();
          }
                    else{
			 $j('#muac_div').show();
                    }
			if(patientage &lt;0 || patientage &gt;18){
                $j('#weight_percentile_div').hide();
			}
            else{
				$j('#weight_percentile_div').show();
                              
            <ifMode mode="VIEW" include="false">
                 $j("#centile_dropdown").show();
                 $j("#weight_percentile").hide();
           </ifMode>

           <ifMode mode="VIEW">
               $j("#centile_dropdown").hide();
               $j("#weight_percentile").show();
            </ifMode>
			}

<ifMode mode="EDIT">

var x = getValue('weight_percentile.value')
document.getElementById("centile_dropdown").value = x;
</ifMode>

if(patientage &lt;0 || patientage &gt;15){
                $j('#exposure_point1_div').hide();
			}
            else{
				$j('#exposure_point1_div').show();
            }

        });
		
		beforeSubmit.push(function() {
