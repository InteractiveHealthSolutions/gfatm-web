                                 document.getElementById("visit_hospital").style.display = "block";

                           }
                        else{
                                  document.getElementById("plan_continue_medication_high").style.display = "block";
                                 document.getElementById("plan_continue_medication_low").style.display = "block";
                                 document.getElementById("plan_discontinue_medication").style.display = "block";
                                 document.getElementById("plan_cxr_monitoring_procedure").style.display = "block";
                                 document.getElementById("plan_new_medication").style.display = "block";
                                 document.getElementById("plan_change_dosage").style.display = "block";
                                 document.getElementById("plan_ancillary_drugs").style.display = "block";
                                 document.getElementById("plan_expert_consultation").style.display = "block";
                                 document.getElementById("plan_treatment_suggested").style.display = "none";
                                 document.getElementById("visit_hospital").style.display = "none";


                         }
                });


 });

function doRest(){
			jQuery.ajax({
				url: "/openmrs/ws/rest/v1/location?v=custom:(id)&amp;q=IBEX-KHI",
				dataType: 'json'
				}).done(function(data ) {
					locId = data.results[0].id;
			});
		}

