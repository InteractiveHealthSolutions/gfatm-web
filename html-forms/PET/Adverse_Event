jQuery(function() {

getField('pet_regimen.value').change(function() {
                 var val1= getValue('pet_regimen.value');

                 document.getElementById("div_isoniazid").style.display = "none";
                 document.getElementById("div_rifapentine").style.display = "none"    

                 document.getElementById("div_levofloxacin").style.display = "none";
                 document.getElementById("div_ethionamide").style.display = "none";
                         
                             if(val1 == '1679'){
                                      document.getElementById("div_isoniazid").style.display = "block";
                                      document.getElementById('div_rifapentine').innerHTML = "";
                                      document.getElementById('div_levofloxacin').innerHTML = "";
                                      document.getElementById('div_ethionamide').innerHTML = "";

                             }
                            else if(val1 == '164241'){
                                     document.getElementById("div_isoniazid").style.display = "block";
                                     document.getElementById("div_rifapentine").style.display = "block";
                                      document.getElementById('div_levofloxacin').innerHTML = "";
                                      document.getElementById('div_ethionamide').innerHTML = "";
                             }
                            else if(val1 == '164242'){
                                    document.getElementById("div_levofloxacin").style.display = "block";
                                    document.getElementById("div_ethionamide").style.display = "block";
                                    document.getElementById('div_isoniazid').innerHTML = "";
                                    document.getElementById('div_rifapentine').innerHTML = "";
                            }
});

getField('plan_change_dosage.value').change(function() {
                        var checked= getValue('plan_change_dosage.value');
                        if(checked == '164320'){
                             document.getElementById("div_regimen").style.display = "block";
                             var val1= getValue('pet_regimen.value');
                             if(val1 == '1679'){
                                      document.getElementById("div_isoniazid").style.display = "block";
                                      document.getElementById('div_rifapentine').innerHTML = "";
                                      document.getElementById('div_levofloxacin').innerHTML = "";
                                      document.getElementById('div_ethionamide').innerHTML = "";
                             }
                            else if(val1 == '164241'){
                                     document.getElementById("div_isoniazid").style.display = "block";
                                     document.getElementById("div_rifapentine").style.display = "block";
                                     document.getElementById('div_levofloxacin').innerHTML = "";
                                     document.getElementById('div_ethionamide').innerHTML = "";
                             }
                            else if(val1 == '164242'){
                                    document.getElementById("div_levofloxacin").style.display = "block";
                                    document.getElementById("div_ethionamide").style.display = "block";
                                    document.getElementById('div_isoniazid').innerHTML = "";
                                    document.getElementById('div_rifapentine').innerHTML = "";

                            }

                         }
                         else{
                             document.getElementById("div_regimen").style.display = "none";
                            document.getElementById("div_isoniazid").style.display = "none";
                            document.getElementById("div_rifapentine").style.display = "none";
                            document.getElementById("div_levofloxacin").style.display = "none";
                            document.getElementById("div_ethionamide").style.display = "none";

                            document.getElementById('div_isoniazid').innerHTML = "";
                            document.getElementById('div_rifapentine').innerHTML = "";
                            document.getElementById('div_levofloxacin').innerHTML = "";
                            document.getElementById('div_ethionamide').innerHTML = "";
                          } 

                });

getField('plan_ancillary_drugs.value').change(function() {
