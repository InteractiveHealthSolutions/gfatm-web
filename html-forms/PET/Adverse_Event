jQuery(function() {

getField('pet_regimen.value').change(function() {
                 var val1= getValue('pet_regimen.value');

                 document.getElementById("div_isoniazid").style.display = "none";
                 document.getElementById("div_rifapentine").style.display = "none"    

                 document.getElementById("div_levofloxacin").style.display = "none";
                 document.getElementById("div_ethionamide").style.display = "none";
                         
                             if(val1 == '1679'){
                                      document.getElementById("div_isoniazid").style.display = "block";
                                      document.getElementById('div_rifapentine').innerHTML = "";
                                      document.getElementById('div_levofloxacin').innerHTML = "";
                                      document.getElementById('div_ethionamide').innerHTML = "";

                             }
                            else if(val1 == '164241'){
                                     document.getElementById("div_isoniazid").style.display = "block";
                                     document.getElementById("div_rifapentine").style.display = "block";
                                      document.getElementById('div_levofloxacin').innerHTML = "";
                                      document.getElementById('div_ethionamide').innerHTML = "";
                             }
                            else if(val1 == '164242'){
                                    document.getElementById("div_levofloxacin").style.display = "block";
                                    document.getElementById("div_ethionamide").style.display = "block";
                                    document.getElementById('div_isoniazid').innerHTML = "";
                                    document.getElementById('div_rifapentine').innerHTML = "";
                            }
});

getField('plan_change_dosage.value').change(function() {
                        var checked= getValue('plan_change_dosage.value');
                        if(checked == '164320'){
                             document.getElementById("div_regimen").style.display = "block";
                             var val1= getValue('pet_regimen.value');
                             if(val1 == '1679'){
                                      document.getElementById("div_isoniazid").style.display = "block";
                                      document.getElementById('div_rifapentine').innerHTML = "";
                                      document.getElementById('div_levofloxacin').innerHTML = "";
                                      document.getElementById('div_ethionamide').innerHTML = "";
                             }
                            else if(val1 == '164241'){
                                     document.getElementById("div_isoniazid").style.display = "block";
                                     document.getElementById("div_rifapentine").style.display = "block";
                                     document.getElementById('div_levofloxacin').innerHTML = "";
                                     document.getElementById('div_ethionamide').innerHTML = "";
                             }
                            else if(val1 == '164242'){
                                    document.getElementById("div_levofloxacin").style.display = "block";
                                    document.getElementById("div_ethionamide").style.display = "block";
                                    document.getElementById('div_isoniazid').innerHTML = "";
                                    document.getElementById('div_rifapentine').innerHTML = "";

                            }

                         }
                         else{
                             document.getElementById("div_regimen").style.display = "none";
                            document.getElementById("div_isoniazid").style.display = "none";
                            document.getElementById("div_rifapentine").style.display = "none";
                            document.getElementById("div_levofloxacin").style.display = "none";
                            document.getElementById("div_ethionamide").style.display = "none";

                            document.getElementById('div_isoniazid').innerHTML = "";
                            document.getElementById('div_rifapentine').innerHTML = "";
                            document.getElementById('div_levofloxacin').innerHTML = "";
                            document.getElementById('div_ethionamide').innerHTML = "";
                          } 

                });

getField('plan_ancillary_drugs.value').change(function() {
                        var checked= getValue('plan_ancillary_drugs.value');
                        if(checked == '164321'){
                             document.getElementById("div_ancillary_drug").style.display = "block";
                             document.getElementById("div_new_medication_duration").style.display = "block";

                   
                         }
                         else{
                             document.getElementById("div_ancillary_drug").style.display = "none";
                             document.getElementById("div_new_medication_duration").style.display = "none";
                                    document.getElementById('div_ancillary_drug').innerHTML = "";
                                    document.getElementById('div_ancillary_drug_duration').innerHTML = "";
                          } 
                });

getField('plan_new_medication.value').change(function() {
                        var checked= getValue('plan_new_medication.value');
                        if(checked == '164318'){
                             document.getElementById("div_new_medication_duration").style.display = "block";
                             document.getElementById("div_new_medication").style.display = "block";

                         }
                         else{
                             document.getElementById("div_new_medication").style.display = "none";
                             document.getElementById("div_new_medication_duration").style.display = "none";
                                    document.getElementById('div_new_medication').innerHTML = "";
                                    document.getElementById('div_new_medication_duration').innerHTML = "";
                         }
                });

getField('plan_discontinue_medication.value').change(function() {
                        var checked= getValue('plan_discontinue_medication.value');
                        if(checked == '164316'){
                             document.getElementById("div_discontinue_reason").style.display = "block";
                             document.getElementById("div_discontinue_duration").style.display = "block";

                         }
                         else{
                             document.getElementById("div_discontinue_reason").style.display = "none";
                             document.getElementById("div_discontinue_duration").style.display = "none";
                             document.getElementById('div_discontinue_reason').innerHTML = "";
                             document.getElementById('div_discontinue_duration').innerHTML = "";
                         }
                });

getField('cb_other_adverse_event.value').change(function() {
                        var checked= getValue('cb_other_adverse_event.value');
                        if(checked == '164294')
                             document.getElementById("div_other_adverse_event").style.display = "block";

                         else{
                             document.getElementById("div_other_adverse_event").style.display = "none";
                                    document.getElementById('div_other_adverse_event').innerHTML = "";

}
                });

getField('encounter_location.value').change(function() {
                        var locationId = getValue('encounter_location.value');
                        if(locationId == locId){
                                 document.getElementById("plan_continue_medication_high").style.display = "none";
                                 document.getElementById("plan_continue_medication_low").style.display = "none";
                                 document.getElementById("plan_discontinue_medication").style.display = "none";
                                 document.getElementById("plan_cxr_monitoring_procedure").style.display = "none";
                                 document.getElementById("plan_new_medication").style.display = "none";
                                 document.getElementById("plan_change_dosage").style.display = "none";
                                 document.getElementById("plan_ancillary_drugs").style.display = "none";
                                 document.getElementById("plan_expert_consultation").style.display = "block";
                                 document.getElementById("plan_treatment_suggested").style.display = "block";
                                 document.getElementById("visit_hospital").style.display = "block";

                           }
                        else{
                                  document.getElementById("plan_continue_medication_high").style.display = "block";
                                 document.getElementById("plan_continue_medication_low").style.display = "block";
                                 document.getElementById("plan_discontinue_medication").style.display = "block";
                                 document.getElementById("plan_cxr_monitoring_procedure").style.display = "block";
                                 document.getElementById("plan_new_medication").style.display = "block";
                                 document.getElementById("plan_change_dosage").style.display = "block";
                                 document.getElementById("plan_ancillary_drugs").style.display = "block";
                                 document.getElementById("plan_expert_consultation").style.display = "block";
                                 document.getElementById("plan_treatment_suggested").style.display = "none";
                                 document.getElementById("visit_hospital").style.display = "none";


                         }
                });


 });

function doRest(){
			jQuery.ajax({
				url: "/openmrs/ws/rest/v1/location?v=custom:(id)&amp;q=IBEX-KHI",
				dataType: 'json'
				}).done(function(data ) {
					locId = data.results[0].id;
			});
		}

