                             document.getElementById("div_discontinue_duration").style.display = "block";

                         }
                         else{
                             document.getElementById("div_discontinue_reason").style.display = "none";
                             document.getElementById("div_discontinue_duration").style.display = "none";
                         }

                        var checked1= getValue('plan_ancillary_drugs.value');
                        if(checked1 == '164321'){
                             document.getElementById("div_ancillary_drug").style.display = "block";
