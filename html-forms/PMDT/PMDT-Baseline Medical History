					setValue('radiological_evidence.value', "No");
					histopathologyEvidence.show();
					radiologicalEvidence.show();
					
					
				}
			});
			
			 
			getField('drug_resistance_profile.value').change(function() {
                
				if(getValue('drug_resistance_profile.value')== 165028){
				
