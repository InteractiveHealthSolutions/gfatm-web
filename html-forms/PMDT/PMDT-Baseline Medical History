					treatmentOutcomeDrtbContact.hide();
					
				}
			});
			
			
			getField('index_patient_drtb_treatment.value').change(function() {
                
				if(getValue('index_patient_drtb_treatment.value')== 1065){
						
					tbRegimen.show();
