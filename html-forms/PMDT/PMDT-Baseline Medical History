			getField('cough_duration.value').change(function() {
				setValue('cough_duration.error','');
				if(getValue('cough_duration.value').length &gt; 2){
					getField('cough_duration.error').html('Value can not be greater than 2 digits').show();
					
				}
				else {
					getField('cough_duration.error').html('').hide();
				}
			});
			
			getField('fever_duration.value').change(function() {
				setValue('fever_duration.error','');
				
				if(getValue('fever_duration.value').length &gt; 2){
					getField('fever_duration.error').html('Value can not be greater than 2 digits').show();
					
				}
				else {
					getField('fever_duration.error').html('').hide();
				}
			});



			
			
			jQuery(":checkbox").click(function(){


				var $this = jQuery(this);
				var checkBoxId = jQuery(this).closest('span').attr('id')
				
				if($this.is(":checked") &amp;&amp; checkBoxId == "other_tb_drug") {
						tbRegimenOther.show();
				}
				else if(checkBoxId == "other_tb_drug") {
						tbRegimenOther.hide();
					
				}
				
				
				if($this.is(":checked") &amp;&amp; checkBoxId == "other_allergy_drug") {
					allergyDrugOther.show();
				}
				else if(checkBoxId == "other_allergy_drug"){
						allergyDrugOther.hide();
						setValue('known_allergy_drug_other.value', "");
						
					
				}
				
				
				if($this.is(":checked") &amp;&amp; checkBoxId == "extra_pulmonary") {
					setValue('extra_pulmonary_site.value', "Other");
					
					extraPulmonarySite.show();
					// since extrapulmonary is Other, so enable other field
					extraPulmonarySiteOther.show();
					
				}
				else if(checkBoxId == "extra_pulmonary") {
						extraPulmonarySite.hide();
						setValue('extra_pulmonary_site_other.value', "");
						
						extraPulmonarySiteOther.hide();

				}
				
				//
				if($this.is(":checked") &amp;&amp; checkBoxId == "other_mtb_confirmation") {
					
					
					mtbConfirmationMethodOther.show();
					
				}
				else if(checkBoxId == "other_mtb_confirmation") {
						
						setValue('mtb_confirmation_method_other.value', "");
						
						mtbConfirmationMethodOther.hide();
					
				}
				
			});
			
		});
		
		beforeSubmit.push(function() {
        
			setValue('amikacin.error','');
            setValue('allergy_amikacin.error','');
			setValue('treatment_outcome_drtb_contact.error','');
			setValue('index_patient_drtb_treatment.error','');
			
			setValue('cough_type.error','');
			setValue('cough_duration.error','');
			setValue('cough_duration_unit.error','');
			
			setValue('fever_duration.error','');
			setValue('fever_duration_unit.error','');
			
			setValue('pulmonary.error','');
			setValue('extra_pulmonary_site.error','');
			setValue('extra_pulmonary_site_other.error','');
			
			// for mtb confirmation obsgroup
			setValue('smear.error','');
			
			setValue('mtb_confirmation_method_other.error','');
			
			// check if drug_resistance_profile = "Confirmed drug resistant TB"
			setValue('drug_resistant_profile_class.error','');
			
			// enable if  "mtb_confirmation" = "Bacteriologically confirmed"
			setValue('rif_res_date.error','');
			
			// Show if "mtb_confirmation" = Not confirmed, clinically diagnosed
			setValue('histopathology_evidence.error','');
			setValue('radiological_evidence.error','');
			
			setValue('other_assessment_reason.error','');
		
			var valid=true;
		
			if(getValue('dr_tb_contact.value')==1065 &amp;&amp; 	getValue('index_patient_drtb_treatment.value')=='') {
				   getField('index_patient_drtb_treatment.error').html('Please select a value.').show();
				   valid=false;
				   
			}
			else {
				   getField('index_patient_drtb_treatment.error').html('').hide();
			}
		
			//=============================================
		
				
			if (jQuery("#display_tb_regimen_child input[type=checkbox]:checked").length > 0)
			{
				tb_regimen_check = true;
			}
			else
			{
				tb_regimen_check = false;
			}
	
			if(getValue('index_patient_drtb_treatment.value')==1065 &amp;&amp; 	tb_regimen_check == false) {
				   getField('amikacin.error').html('Please select a value.').show();
				   valid=false;
				   
			}
			else {
				   getField('amikacin.error').html('').hide();
			}
			
			// ========================================
			
			
			if(getValue('index_patient_drtb_treatment.value')==1065 &amp;&amp; 	getValue('treatment_outcome_drtb_contact.value')=='') {
				   getField('treatment_outcome_drtb_contact.error').html('Please select a value.').show();
				   valid=false;
				   
			}
			else {
				   getField('treatment_outcome_drtb_contact.error').html('').hide();
			}
			
			//========================================
			
			
			if (jQuery("#display_known_allergy_drug_list input[type=checkbox]:checked").length > 0)
			{
				drug_allergy_check = true;
			}
			else
			{
				drug_allergy_check = false;
			}
			
			if(getValue('drug_allergies.value')==1065 &amp;&amp; 	drug_allergy_check == false) {
				   getField('allergy_amikacin.error').html('Please select a value.').show();
				   valid=false;
				   
			}
			else {
				   getField('allergy_amikacin.error').html('').hide();
			}
			
			
			if(getValue('cough.value')== 1065 &amp;&amp; getValue('cough_type.value')=='') {
				   getField('cough_type.error').html('Please select a value.').show();
				   valid=false;
				   
			}
			else {
				   getField('cough_type.error').html('').hide();
			}
			
			
			if(getValue('cough.value')== 1065 &amp;&amp; 	getValue('cough_duration.value')=='') {
				   getField('cough_duration.error').html('Please input a value.').show();
				   valid=false;
				   
			} else if(getValue('cough_duration.value').length &gt; 2){
				getField('cough_duration.error').html('Value can not be greater than 2 digits').show();
                valid=false;
            }
			else {
				   getField('cough_duration.error').html('').hide();
			}
			
			
			if(getValue('cough.value')== 1065 &amp;&amp; 	getValue('cough_duration_unit.value')=='') {
				   getField('cough_duration_unit.error').html('Please select a value.').show();
				   valid=false;
				   
			}
			else {
				   getField('cough_duration_unit.error').html('').hide();
			}
			
			//================================================
			if(getValue('fever.value')== 1065 &amp;&amp; 	getValue('fever_duration.value')=='') {
				   getField('fever_duration.error').html('Please input a value.').show();
				   valid=false;
				   
			} 
			else if(getValue('fever_duration.value').length &gt; 2){
				getField('fever_duration.error').html('Value can not be greater than 2 digits').show();
                valid=false;
            }
			else {
				   getField('fever_duration.error').html('').hide();
			}
			
			if(getValue('fever.value')== 1065 &amp;&amp; 	getValue('fever_duration_unit.value')=='') {
				   getField('fever_duration_unit.error').html('Please select a value.').show();
				   valid=false;
				   
			}
			else {
				   getField('fever_duration_unit.error').html('').hide();
			}
			
			// =============================================================
			
			
			if (jQuery("#display_disease_site_list input[type=checkbox]:checked").length > 0)
			{
				extra_pulmonary_check = true;
			}
			else
			{
				extra_pulmonary_check = false;
			}
			
			if(extra_pulmonary_check == false) {
				   getField('pulmonary.error').html('Please select a value.').show();
				   valid=false;
				   
			}
			else {
				   getField('pulmonary.error').html('').hide();
			}
			
			// =============================================================
			
			
			if(getField("extra_pulmonary.value").prop('checked')== true &amp;&amp; getValue('extra_pulmonary_site.value')== '') {
				   getField('extra_pulmonary_site.error').html('Please select a value.').show();
				   valid=false;
				   
			}
			else {
				   getField('extra_pulmonary_site.error').html('').hide();
			}
			
			if(getField("extra_pulmonary.value").prop('checked')== true &amp;&amp; getValue('extra_pulmonary_site.value')== 164429 &amp;&amp; 	getValue('extra_pulmonary_site_other.value')=='') {
				   getField('extra_pulmonary_site_other.error').html('Please input a value.').show();
				   valid=false;
				   
			}
			else {
				   getField('extra_pulmonary_site_other.error').html('').hide();
			}
			
			
			//===============================================
			
				
			if (jQuery("#display_mtb_confirmation_method_list input[type=checkbox]:checked").length > 0)
			{
				mtb_confirmation_check = true;
			}
			else
			{
				mtb_confirmation_check = false;
			}
	
			if(getValue('mtb_confirmation.value')== 164996 &amp;&amp; 	mtb_confirmation_check == false) {
				   getField('smear.error').html('Please select a value.').show();
				   valid=false;
				   
			}
			else {
				   getField('smear.error').html('').hide();
			}
			
			//=======================================================
			
			if(getValue('mtb_confirmation.value')== 164996 &amp;&amp; getField("other_mtb_confirmation.value").prop('checked')== true &amp;&amp; getValue('mtb_confirmation_method_other.value')=='') {
				
				   getField('mtb_confirmation_method_other.error').html('Please input a value.').show();
				   valid=false;
				   
			}
			else {
				   getField('mtb_confirmation_method_other.error').html('').hide();
			}
			
			// ====================================  
			if(getValue('drug_resistance_profile.value')== 165028 &amp;&amp; getValue('drug_resistant_profile_class.value')=='') {
				
				   getField('drug_resistant_profile_class.error').html('Please select a value.').show();
				   valid=false;
			}
			else {
				   getField('drug_resistant_profile_class.error').html('').hide();
			}
			
			// =======================
			if(getValue('mtb_confirmation.value')== 164996 &amp;&amp; getValue('rif_res_date.value')=='') {
					
				   getField('rif_res_date.error').html('Please select a date.').show();
				   valid=false;
				   
			}
			else {
				   getField('rif_res_date.error').html('').hide();
			}
			
			// ================================
			if(getValue('mtb_confirmation.value')== 164997 &amp;&amp; getValue('histopathology_evidence.value')=='') {
					
				   getField('histopathology_evidence.error').html('Please select a value.').show();
				   valid=false;
				   
			}
			else {
				   getField('histopathology_evidence.error').html('').hide();
			}
			
			// =======================================
			if(getValue('mtb_confirmation.value')== 164997 &amp;&amp; getValue('radiological_evidence.value')=='') {
					
				   getField('radiological_evidence.error').html('Please select a value.').show();
				   valid=false;
				   
			}
			else {
				   getField('radiological_evidence.error').html('').hide();
			}
			
			// ============================================== 
			if(getValue('reason_next_assessment.value')== 165037 &amp;&amp; getValue('other_assessment_reason.value')=='') {
					
				   getField('other_assessment_reason.error').html('Please enter the reason.').show();
				   valid=false;
				   
			}
			else {
				   getField('other_assessment_reason.error').html('').hide();
			}
			
			
			if(valid==true){
				
				if(getValue('eligible_for_treatment.value') == 166041) {
					alert("Please fill Expert Panel Review form for this patient.");
				}
				
				var timeTakenDiv = $j("#time_taken_div");
				
				var endDate = new Date();
                var timeDiff = Math.abs(endDate.getTime() - startDate.getTime());
				var diffSec = timeDiff/1000;
				
				timeTakenDiv.show();
				setValue("time_taken.value",diffSec);
				timeTakenDiv.hide();
                return true;
			}
			else{
			  return false;
			}
	
		});
		
		
		

</script>
</htmlform>